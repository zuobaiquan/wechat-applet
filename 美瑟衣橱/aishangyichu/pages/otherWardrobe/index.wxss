.container .no{

}
.container .no .title{
    color: #FF5669;
    font-size: 14px;
    padding-top: 280rpx;
    text-align: center;
}
.container .no .line{
    width:312rpx;
    height:331rpx;
    margin: 0 auto;
    transform: translateX(50%);
    margin-top: 30rpx;
}
.container .no .line image{
    width: 100%;
    height: 100%;
    display:block;
}
/*衣橱列表*/
.wardrobe-list{
    display:flex;
    align-items: center;
    flex-wrap: wrap;
    padding-left: 61rpx;
    padding-bottom: 40rpx;
}
.wardrobe-list .item{
    width:286rpx;
    position:relative;
    z-index: 5;
    border-radius: 0 0 20rpx 20rpx;
    margin-right: 56rpx;
    margin-top: 0rpx;
}
.wardrobe-list .item:nth-of-type(1),.wardrobe-list .item:nth-of-type(2){
    margin-top: 40rpx;
}
.wardrobe-list .item .image{
    width: 100%;
    height: 286rpx;
    background-size: cover;
    background-repeat: no-repeat;
    background-clip: content-box;
    background-origin: content-box;
    background-position: center top;
    border-radius: 20rpx;
}
.wardrobe-list .item .image .img{
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
}
.wardrobe-list .item .news{
    padding: 0 0 0 10rpx;
    overflow: hidden;
}
.wardrobe-list .item .news .label{
    padding: 12rpx 0;
    font-size: 14px;
    color:#333;
}
.wardrobe-list .item .news .view{
    color: #999;
    font-size: 14px;
}
.wardrobe-list .item .box-image{
    position: relative;
}
.wardrobe-list .item .impression{
    position: absolute;
    bottom: -10rpx;
    left: 0;
    right: 0;
    margin: 0 auto;
    height: 100rpx;
    border-radius: 20rpx;
    background-color: rgba(255,86,105,0.2);
    width: 253rpx;
    z-index: -2;
}
/*悬浮按钮*/
.container .button .add{
    position:fixed;
    right: 25rpx;
    bottom:270rpx;
    z-index: 10;
}
.container .button .leave{
    position: fixed;
    right:25rpx;
    bottom:70rpx;
    z-index: 10;
}
.container .button .text{
    color:#666;
    font-size: 12px;
    padding-top: 25rpx;
    text-align: center;
}
.container .button .add .icon,.container .button .leave .icon{
    width:81rpx;
    height:81rpx;
}
.container .button .add .icon image,.container .button .leave .icon image{
    width: 100%;
    height: 100%;
    display:block;
}

