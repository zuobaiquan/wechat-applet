/*}*/
.container .tag-popup .content{
    background-color: #fff;
    width: 560rpx;
    padding-bottom: 35rpx;
}
.container .tag-popup .tag-button{
    display:flex;
    justify-content: flex-end;
    padding-right: 20rpx;
}
.container .tag-popup .tag-close{
    display:flex;
    justify-content: center;
    align-items: center;
    padding: 25rpx 20rpx 0  0;
}
.container .tag-popup .tag-close view{
    background-color:#ccc;
    width: 2px;
    height: 30rpx;
    transform:rotate(45deg) translateY(-1rpx);
}
.container .tag-popup .tag-close .line{
    transform:rotate(-45deg) translateY(-1rpx);
}
.container .tag-popup .input{
    display:flex;
    align-items: center;
    width: 465rpx;
    margin: 50rpx auto 0;
    border-bottom: 1px solid #EEEEEE;
    padding-bottom: 15rpx;
}
.container .tag-popup .input view{
    width: 4rpx;
    height: 58rpx;
    background-color:#FF5669;
}
.container .tag-popup .input input{
    display:block;
    flex-grow: 1;
    padding: 0 22rpx;
    color:#999999;
    font-size: 15px;
}
.container .tag-popup button{
    width: 375rpx;
    height: 80rpx;
    line-height: 80rpx;
    border:none;
    background-color: #FF5669;
    color: #fff;
    font-size: 14px;
    margin:85rpx auto 0;
    display:block;
}