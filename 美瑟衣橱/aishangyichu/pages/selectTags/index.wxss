.container{
    padding-top:18rpx;
}
.container .button{
    display:flex;
    justify-content:space-between;
    align-items: center;
}
.container .button view{
    border:none;
    display: block;
    background-color:#FF5669;
    color:#fff;
    padding: 15rpx 24rpx;
    margin-right:30rpx;
    font-size: 14px;
    border-radius: 6rpx;
}
.container .button .cancel{
    background-color:transparent;
    color: #101010;
    font-size: 14px;
}

.container .create{
    background-color:#fff;
    padding:20rpx;
    padding-left: 29rpx;
    padding-bottom: 0rpx;
    display:flex;
    align-items: center;
    flex-wrap: wrap;
    margin-top:26rpx;
}
.container .create view{
    padding:10rpx 34rpx;
    color:#999999;
    font-size: 13px;
    border: 1rpx solid #999999;
    border-radius: 4rpx;
    margin-bottom: 20rpx;
    position: relative;
    margin-right: 20rpx;
}
.container .create .border-red,.container .create .check{
    border: 1px solid #FF5669;
    color:#FF5669;
}
.container .create view>image{
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    top: -15rpx;
    right: -15rpx;
}
.container .create .addTag{
    width: 60rpx;
    height:60rpx;
    margin-bottom: 20rpx;
    display: block;
}
/*类型*/
.container .type{

}
.container .type .title{
    display:flex;
    align-items: center;
    background-color:#fff;
    margin-left: 27rpx;
    padding: 10rpx 0 10rpx 20rpx;
    border-radius: 30rpx;
    box-sizing: border-box;
    width: 125rpx;
    margin-top: 40rpx;
}
.container .type .title view{
    color:#333333;
    font-size: 12px;
}
.container .type .title .dot{
    width: 14rpx;
    height: 14rpx;
    border-radius: 50%;
    background-color: #FF5669;
    margin-right:20rpx;
}
.container .type .list{
    background-color:#fff;
    padding: 30rpx 0rpx 10rpx 0;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    margin-top:40rpx;
}
.container .type .list view{
    padding: 10rpx 30rpx;
    color:#999;
    font-size: 13px;
    border: 1rpx solid #999;
    border-radius: 4rpx;
    margin-left:29rpx;
    margin-bottom: 20rpx;
}
.container .type .list .active{
    padding: 10rpx 30rpx;
    color:#FF5669;
    font-size: 13px;
    border: 1rpx solid #FF5669;
    border-radius: 4rpx;
}
