}
.address-list{
  background: #fff;
  margin-bottom: 20rpx;
  padding: 20rpx;
}
.address-list .user-info{
  display: flex;
  justify-content: space-between;
}
.address-list .user-info text{
  font-size: 28rpx;
}
.address-list .address-name{
  color:#999;
}
.address-list .address-operator{
  display: flex;
  justify-content: space-between;
  border-top:1rpx solid #ddd;
}
