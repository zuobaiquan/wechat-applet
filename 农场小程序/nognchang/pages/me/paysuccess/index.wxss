/* pages/me/paysuccess/index.wxss */
.container .icon-box{
    width:100%;
    padding: 136rpx 0 0;
    text-align: center;
}
.icon-box .icon{
    width:107rpx;
    height: 107rpx;
    margin:0 auto 50rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.icon-box .text{
    font-size: 36rpx;
    color:#000000;
}
.container .box-list{
    width: 70%;
    margin:0 auto;
    display: flex;
    justify-content: space-around;
}
.box-list text{
    margin-top:92rpx;
    padding: 0 36rpx;
    height: 70rpx;
    line-height: 70rpx;
    border:1rpx solid #96CB2C;
    border-radius: 35rpx;
    text-align: center;
    color:#96CB2C;
    font-size: 28rpx;
}
