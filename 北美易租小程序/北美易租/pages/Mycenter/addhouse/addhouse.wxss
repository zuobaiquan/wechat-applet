.add-houseimg{
  width:100%;
  height:340rpx;
  background:rgba(0,0,0,0.9);
  position: relative;
  
}
.add-houseimg .house-img{
  width:100%;
  height:340rpx;
}
.add-houseimg .photo-img{
  position: absolute;
  top:50%;
  left:50%;
  width:120rpx;
  height:120rpx;
  z-index:99;
  transform: translate(-50%,-50%);
}
.add-houseimg .house-imgtitle{
  position: absolute;
  bottom:60rpx;
  margin-left:50%;
  transform: translate(-50%,0);
  font-size:32rpx;
  color:#ffffff;
}

.house-itemlist{
  margin:0 70rpx;
  overflow: hidden;
}
.item-box{
  position: relative;
  height:128rpx;
  line-height: 200rpx;
  border-bottom:2rpx solid #999999;
}
.item-list .item-left{
  font-size:32rpx;
  color:#4b474c;
}
.house-itemlist .item-input{
  margin-left:6rpx;
  width:310rpx;
}

.item-list .item-checked{
  position: absolute;
  bottom:12rpx;
  right:0;
  width:32rpx;
  height:32rpx;
}