/* rentdetail.wxss */
swiper{
  height:482rpx;
}
.slide-image{
  width:100%;
  height:482rpx;
}
.publish-top{
  height:auto;
  width:100%;
}
.save-lookimg{
  height:70rpx;
}
.save-tip{
  display: inline-block;
  margin:20rpx 0 0 34rpx;
  font-size:32rpx;
  height:44rpx;
  line-height: 44rpx;
}
.save-img{
  margin:10rpx 34rpx 0 0;
  float:right;
  background:#f47a20;
  border-radius:10rpx;
  width:184rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  color:#ffffff;
}
.publish-top .top-con{
  margin:0 34rpx;
  height:110rpx;
  line-height: 150rpx;
  font-size:26rpx;
  color:#999999;
}
.top-con .publish-avator{
  margin:40rpx 20rpx 0 0;
  width:70rpx;
  height:70rpx;
  border-radius: 50%;
  float: left;
}
.house-info{
  margin:0 34rpx;
  width:91%;
  overflow:hidden;
}
.house-info text{
  display: inline-block;
}
.house-info .info-addr{
  margin-top:12rpx;
  font-size:28rpx;
}
.house-info .info-left{
   float: left;
   width:64%;
}
.info-left .info-name{
  font-weight: bold;
  font-size:36rpx;
}
.house-info .info-right{
  float: right;
  width:36%;
  text-align: right;
}
.info-right .info-price{
  font-size:36rpx;
}
.publish-right{
  float: right;
}
.publish-right .rightcollect{
  margin-left:14rpx;
  width:40rpx;
  height:38rpx;
  vertical-align: middle;
}
.detail-item{
  margin:0 34rpx;
  border-bottom:2rpx solid #dfdfdf;
}
.addr-detail{
  overflow: hidden;
  padding:36rpx 0;
}
.addr-detail .addr-name{
  width:220rpx;
  float: left;
}
.addr-detail .house-addr{
  float: left;
  width:420rpx;
}
.rent-ammout{
  color:#F47C23;
}
.detail-item .item-name{
  display: inline-block;
  width:220rpx;
  height:120rpx;
  line-height: 120rpx;
  font-weight:bold;
}
.extrl-contract{
  margin-top:40rpx;
  font-size:40rpx;
  font-weight:bold;
}
.detail-sheshi .extrl-contract{
  margin:40rpx 0;
}

.extral-detail{
  display: inline-block;
  line-height:40rpx;
  margin:40rpx 0;
}
.contract-sp{
  margin-bottom: 222rpx;
}
.detail-bottom{
  margin:180rpx 0  20rpx;
}

.detail-bottom .btn-phone{
  width:84%;
  margin:0 auto;
  background:#f47a20;
  border-radius:10rpx;
  height:70rpx;
  line-height:70rpx;
  font-size:32rpx;
  color:#ffffff;
  text-align: center;
}
.detail-share{
  height:auto;
  width: 96rpx;
  text-align: center;
}
.detail-share .share-img{
  width:64rpx;
  height:64rpx;
}
.detail-share .btn-share{
  font-size:24rpx;
  color:#01c605;
}
.house-facity{
  width:33.33%;
  height:160rpx;
  float: left;
  text-align: center;
}
.sheshi-sp{
  margin-bottom: 0;
}
.sheshi-sp .detail-sheshi{
   padding-bottom:40rpx;
}
.box-item3{
  margin:18rpx 0;
  width:33.33%;
  display: inline-block;
  text-align: center;
}
.box-item3 .item3-img{
  width:60rpx;
  height:60rpx;
}
.box-item3 .item3-text{
  display: block;
}