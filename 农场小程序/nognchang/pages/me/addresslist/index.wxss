.container{
  padding-bottom: 100rpx;
}
.address-list{
  background: #fff;
  margin-bottom: 20rpx;

}
.address-list .user-info{
  padding: 20rpx 30rpx 10rpx 30rpx;
  font-size: 30rpx;
  color:#000000;
}
.address-list .user-info .phone{
  margin-left: 30rpx;
}
.address-list .address-name{
  padding:0 30rpx 30rpx 30rpx;
  color:#666666;
  font-size: 26rpx;
}
.address-list .address-name text{
  margin-right: 5rpx;
  color:#FF9600;
}
.address-list .address-operator{
  padding:20rpx 0 20rpx 30rpx;
  display: flex;
  justify-content: space-between;
  border-top:1rpx solid #ddd;
}
.address-list .address-operator .selected{
  margin: -2rpx 24rpx 0 0;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 27rpx;
  height: 27rpx;
  display: inline-block;
  vertical-align: middle;
}
.address-list .address-operator .unselected{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.address-list .address-operator .right{
  font-size: 28rpx;
  color:#333333;
}
.address-list .address-operator .right-box{
  display: inline-block;
  text-align: center;
}
.address-operator .right-box1{
  padding: 0 15rpx;
}
.address-operator .right-box2{
  padding: 0 30rpx 0 15rpx;
}
.address-operator .right .edit-icon{
  margin:-2rpx 17rpx 0 0;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 22rpx;
  height: 23rpx;
  display: inline-block;
  vertical-align: middle;
}
.address-operator .right .delete-icon{
  margin: -2rpx 15rpx 0 0;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 26rpx;
  height: 23rpx;
  display: inline-block;
  vertical-align: middle;
}
.address-operator .right .edit-delete{
  color:#666666;
  font-size: 26rpx;
}
.container .add-new{
  position: fixed;
  bottom: 0;
  left:0;
  right: 0;
  height: 109rpx;
  line-height: 109rpx;
  color:#282828;
  background:#fff;
  text-align: center;
}
.container .add-new text{
  height: 80rpx;
  line-height: 80rpx;
  color:#FFFFFF;
  font-size: 30rpx;
  padding:0 80rpx;
  border-radius: 50rpx;
  display: inline-block;
  background: -webkit-linear-gradient(left,#96C454, #CBE154);
  background: linear-gradient(left,#96C454, #CBE154);
}
