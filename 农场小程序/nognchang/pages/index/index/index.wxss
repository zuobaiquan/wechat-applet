.container{

}
.container .header{
  width: 100%;
  height: 300rpx;
  position: relative;
}
.container .header .icon-person{
  position: absolute;
  top:8rpx;
  left:5rpx;
  height: 53rpx;
  width: 53rpx;
  padding:8rpx;
  z-index: 2;
}
.header .icon-person .icon{
  height: 53rpx;
  width: 53rpx;
  display: inline-block;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.container .slide-image{
  width: 100%;
  height: 300rpx;
}
.slide-image swiper{
  height: 300rpx;
}
.container .item-image{
  width: 100%;
  height: 100%;
}
.product-list{
  padding:37rpx 0 30rpx;
  display: flex;
  justify-content: space-between;
  background: #fff;
}
.product-list .item{
  flex:1;
  text-align: center;
}
.product-list .item .icon{
  width: 55rpx;
  height: 55rpx;
  margin:0 auto;
}
.product-list .item .icon1{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.product-list .item .icon2{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.product-list .item .icon3{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.product-list .item .text{
  margin-top:30rpx;
  font-size: 26rpx;
  color:#333333;
}
.product-add{
  margin-top:20rpx;
  background:#fff;
}
.product-add .tit{
  display: block;
  padding:40rpx 0 10rpx 33rpx;
  color:#333333;
  font-size: 34rpx;
}
.product-add .add-box{
  box-sizing: border-box;
  height: 200rpx;
  width: 100%;
  text-align: center;
  position: relative;
}
.add-box image.addbox-bg{
  width: 100%;
  height: 200rpx;
}
.add-box .add-content{
  position: absolute;
  top:20rpx;
  left:20rpx;
}
.add-content .icon{
  padding:2rpx;
  width: 84rpx;
  height: 84rpx;
  background:url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.add-content text{
  font-size: 28rpx;
  color:#508219;
}
.myproduct-list{
  margin: 30rpx 30rpx 0;
  position: relative;
}
.myproduct-list .list-img{
  display: block;
  width: 100%;
  height: 280rpx;
  border-radius: 8rpx;
}
.myproduct-list .list-box{
  position: absolute;
  bottom: 25rpx;
  left:25rpx;
  border-radius: 25rpx;
  background: rgba(191,191,191,.75);
  font-size: 26rpx;
  color:##333333;
}
.myproduct-list .list-box text{
  padding:14rpx 18rpx;
  font-size: 28rpx;
  color:#000000;
}
