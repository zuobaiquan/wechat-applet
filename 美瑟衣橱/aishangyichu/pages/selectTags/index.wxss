/*是否同步圈子*/
.container .sync{
    background-color:#fff;
    height: 120rpx;
    display:flex;
    align-items: center;
    padding: 0 40rpx;
    justify-content: space-between;
    margin-top:48rpx;
}
.container .sync text{
    display:block;
    color:#333;
    font-size: 13px;
}
.container .sync image{
    display:block;
    width: 100rpx;
    height: 55rpx;
}
/*添加弹窗*/
.container .tag-popup{
    position:fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.6);
    display: flex;
    justify-content: center;
    align-items: center;
}
