.tab-houselist {
  margin:0;
  height:484rpx;
}
.now-house{
  font-size:28rpx;
  color:#999999;
  text-align: center;
  height:50rpx;
  line-height: 90rpx;
}
.new-houselist{
  border: solid #999999;
  border-width: 2rpx 0 2rpx 0;
  text-align: center;
}
.new-box{
  width:200rpx;
  margin: 168rpx auto;
}
.newhouse-add{
  width: 84rpx;
  height:84rpx;
}
.add-morenine{
  clear:both;
  padding:20rpx 0 138rpx;
  text-align: center;
  color:#999999;
}

.house-imgbigbox{
  position:relative;
}
.house-imgbig{
  width:100%;
  height:484rpx;
  object-fit: cover;
}

.house-imgsmall{
  width:312rpx;
  height:200rpx;
  float: left;
  overflow: hidden;
  object-fit: cover;
}
.imgsmall-box{
  position:relative;
  margin:42rpx 0 0 42rpx;
  width:312rpx;
  height:200rpx;
  float: left;
}
.box-delete{
  position: absolute;
  top:0;
  left:0;
  height:54rpx;
  width:54rpx;
  transform:translate(-40%,-40%);
}
.box-bigdelete{
  transform:translate(50%,50%);
}


.addmoresp{
  margin:42rpx 0 0 42rpx;
  border:2rpx solid #4b474c;
}
.addmoresp-img{
  margin-top:44rpx;
}