.container .pay-item{
  padding:25rpx 0 0 35rpx;
  display: flex;
  justify-content:flex-start;
  background: #fff;
}
.container .pay-item text{
  display: block;
}
.container .pay-item .text2{
  margin-top: 10rpx;
}
.pay-item .text-box {
  display: inline-block;
}
.pay-item .text-box .list{
  margin-bottom: 25rpx;
}
.container  .pay-list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  border-top: 1rpx solid #e5e5e5;
}
.container  .pay-list view{
  display: inline-block;
}
.container .icon{
  margin:-5rpx 26rpx 0 0;
  width:42rpx;
  height: 42rpx;
  vertical-align: middle;
}
.container .text{
  font-size: 30rpx;
  color:#000000;
}
.container .text2{
  font-size: 28rpx;
  color:#949494;
}
.pay-list .item-left{
  padding: 35rpx;
}
.input-phone{
  text-align: right;
  padding-right: 35rpx;
}
.pay-list .item-right{
  padding: 35rpx;
  font-size: 30rpx;
  color:#666666;
}
.pay-list .item-right .arrow-right{
  margin-left:20rpx;
}
.pay-item .icon1{
  margin:5rpx 26rpx 0 0;
  background:url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.pay-list .icon2{
  background:url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.pay-list .icon3{
  background:url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.pay-way{
  margin-top:20rpx;
  padding-left: 35rpx;
  background: #fff;
}
.pay-way .tit{
  padding:25rpx 0 25rpx 0;
  display: block;
  color:#333333;
  font-size: 28rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.pay-way .payway-box{
  text-align: center;
  padding:40rpx 0 35rpx;
}
.pay-way .payway-box text{
  display: block
}
.pay-way .payway-box .txt1{
  color:#666666;
  font-size: 28rpx;
}
.pay-way .payway-box .clock{
  margin:-3rpx 17rpx 0 0;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 28rpx;
  height: 28rpx;
  display: inline-block;
  vertical-align: middle;
}
.pay-way .payway-box .txt2{
  padding:20rpx 0;
  color:#FF9600;
  font-size: 60rpx;
  font-weight: bold;
}
.pay-way .payway-box .txt3{
  color:#666666;
  font-size: 24rpx;
}
.pay-way .pay-wechat{
  display: flex;
  justify-content: space-between;
  align-items:center;
  padding:25rpx 35rpx 25rpx 0;
  border-top: 1rpx solid #e5e5e5;
}
.pay-way .pay-wechat .left{
  color:#000000;
  font-size: 30rpx;
}
.pay-way .pay-wechat .left-icon{
  margin:-2rpx 26rpx 0 0;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 48rpx;
  height: 48rpx;
  display: inline-block;
  vertical-align: middle;
}
.pay-way .pay-wechat .right-icon{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 65rpx;
  height: 65rpx;
  display: inline-block;
  vertical-align: middle;
}
