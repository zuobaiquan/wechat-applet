/* rentdetail.wxss */
.publish-top{
  height:auto;
  width:100%;
}
.save-lookimg{
  height:70rpx;
}
.save-tip{
  display: inline-block;
  margin:20rpx 0 0 34rpx;
  font-size:32rpx;
  height:44rpx;
  line-height: 44rpx;
}
.save-img{
  margin:10rpx 34rpx 0 0;
  float:right;
  background:#f47a20;
  border-radius:10rpx;
  width:184rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  color:#ffffff;
}
.publish-top .top-con{
  margin:0 34rpx;
  height:110rpx;
  line-height: 150rpx;
  font-size:26rpx;
  color:#999999;
}
.top-con .publish-avator{
  margin:40rpx 20rpx 0 0;
  width:70rpx;
  height:70rpx;
  border-radius: 50%;
  float: left;
}

.publish-right{
  float: right;
}
.publish-right .rightcollect{
  margin-left:14rpx;
  width:40rpx;
  height:38rpx;
  vertical-align: middle;
}
.detail-item{
  margin:0 34rpx;
  border-bottom:2rpx solid #dfdfdf;
}

.detail-item .item-name{
  display: inline-block;
  width:220rpx;
  height:120rpx;
  line-height: 120rpx;
  font-weight:bold;
}
.extrl-contract{
  margin-top:40rpx;
  font-size:40rpx;
  font-weight:bold;
}
.contract-box{
  margin-bottom:180rpx;
}

.detail-bottom{
  position: fixed;
  bottom: 14rpx;
  right:0 ;left:0;
}
.detail-bottom .btn-phone{
  width:84%;
  margin:0 auto;
  background:#f47a20;
  border-radius:10rpx;
  height:70rpx;
  line-height:70rpx;
  font-size:32rpx;
  color:#ffffff;
  text-align: center;
}
.detail-share{
  height:auto;
  width: 96rpx;
  text-align: center;
}
.detail-share .share-img{
  width:64rpx;
  height:64rpx;
}
.detail-share .btn-share{
  font-size:24rpx;
  color:#01c605;
}