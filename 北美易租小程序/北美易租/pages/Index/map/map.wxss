.search-textcon {
    position: relative;
    margin: 14rpx 30rpx;
}

.search-textcon .serach-icon {
    position: absolute;
    left: 16rpx;
    top: 10rpx;
    width: 34rpx;
    height: 32rpx;
}

.search-textcon .search-text {
  width: 80%;
  border: 1px solid #4b474c;
  border-radius: 10px;
  font-size: 28rpx;
  color: #4b474c;
  height: 62rpx;
  padding-left: 60rpx;
}

.search-list {
  position: absolute;
  right: -10rpx;
  top: 0;
  bottom: 0;
  font-size: 32rpx;
  padding: 15rpx;
}

map {
  position:relative;
  width: 100%;
  top: 10rpx;
  z-index:-1 !important;
}

.hidden-arrow {
  width: 34rpx;
  margin: 0 auto;
  margin-bottom: 16rpx;
  height: 34rpx;
  border: solid #dfdfdf;
  border-width: 6rpx 6rpx 0 0;
  -webkit-transform: rotate(135deg);
  transform: rotate(135deg);
}

.tab-houselist {
  height: 396rpx;
}

.tab-houselist {
  margin: 0;
}

.tab-housedetail .housedetail {
  padding: 0;
  margin: 6rpx 24rpx 0;
}

.map-right {
    top: 78rpx;
}

.shequ {
    display: inline-block;
    width: 80rpx;
    height: 38rpx;
    text-align: center;
    line-height: 38rpx;
    background: #f47a20;
    color: #fff;
    font-size: 24rpx;
    border-radius: 20rpx;
    margin-left: -4rpx;
    margin-right: 10rpx;
    margin-top: -3rpx;
}

.map-tip {
    position: absolute;
    left: 0;
    top: 82rpx;
    width: 100%;
    z-index: 999;
    background: #ffffff;
}

.map-tip view {
    height: 48rpx;
    margin: 20rpx;
    border-bottom: 1px solid #e8e8e8;
    font-size: 26rpx;
    color: #999;
    background: white;
    margin-left: 0;
    margin-right: 0;
    padding-left:40rpx;
    padding-right: 40rpx;
}
.map-tip view:last-child{
    margin-bottom: 0;
}
.map-tip .cur-location {
    font-size: 26rpx;
    color: #333;
    height: 56rpx;
    position: relative;
    padding-left: 80rpx;
}
.search-list image{
    width: 45rpx;
    height: 45rpx;
    position: absolute;
    right: 15rpx;
    top:15rpx;
}

.show-tips{
  position: absolute;
  top:10%;
  left:50%;
  height:120rpx;
  width:240rpx;
  border-radius: 20rpx;
  transform: translate(-50%,-50%);
  background: rgba(17, 17, 17, 0.7);
  color:#ffffff;
  text-align: center;
  padding:60rpx 0;
  z-index:9999 !important;
}
.show-tips .collect-right{
  width:100rpx;
  height:72rpx;
}
.show-tips .collect-error{
  width:74rpx;
  height:74rpx;
}
.show-tips .tips-info{
  margin:20rpx 0;
  display: block;
  color:#ffffff;
  font-size: 32rpx;
}
