page{
    background-color:#fff;
}
.container .block{
 padding-bottom: 16rpx;
}
.container .block .time{
    padding: 32rpx 0 32rpx 30rpx;
    color:#000;
    font-size: 15px;
}
.container .block .image{
    display:flex;
    align-items: center;
    padding-left:7rpx;
    flex-wrap: wrap;
    position: relative;

}
.container .box-imgs{
    position: relative;
}
.container .block .image .edit-delete{
    position: absolute;
    width: 48rpx;
    height: 48rpx;
    left:14rpx;
    top: 14rpx;
}
.container .block .image view{
    background-repeat: no-repeat;
    background-clip:content-box;
    background-origin: content-box;
    background-position: center top;
    width:176rpx;
    height:176rpx;
    margin-left:9rpx;
    margin-bottom:10rpx;
}
.container .block .image .img{
    width: 100%;
    height: 100%;
}
.container .block .label{
    margin-top:17rpx;
    display:flex;
    align-items: center;
    flex-wrap: wrap;
    padding-left: 31rpx;
}
.container .block .label .icon{
    width: 36rpx;
    height: 36rpx;
}
.container .block .label .icon image{
    width: 100%;
    height: 100%;
    display: block;
}
.container .block .label .text text{
    font-size: 12px;
    color: #FF5669;
    padding-left: 19rpx;
}
.container .foot{
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    justify-content: space-around;
    align-items: center;
    background-color: #FF5669;
    width: 100%;
    height:47px;
    box-sizing:border-box;
}
.container .foot .item{
    box-sizing: border-box;
    color: #ffffff;
    font-size: 16px;
    flex-shrink: 0;
}
.container .foot .item image{
    width: 42rpx;
    height: 42rpx;
    display: block;
    margin: 0 auto;
    padding: 16rpx 0 0 0;
}
.container .foot .item text{
    color: #ffffff;
    font-size: 11px;
    text-align: center;
    padding: 6rpx 0 4rpx 0;
    width: 100%;
    display: block;
}
.container .margin-box{
    margin-top: 90rpx;
    height: 1px;
}