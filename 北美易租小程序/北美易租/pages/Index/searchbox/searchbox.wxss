.search-textcon{
  position:relative;
  margin:14rpx 30rpx;
}
.search-textcon .serach-icon{
  position:absolute;
  left:16rpx;
  top:10rpx;
  width:34rpx;
  height:32rpx;
}
.search-textcon .clear-icon{
  position:absolute;
  right:96rpx;
  top:16rpx;
  width:34rpx;
  height:32rpx;
}
.cancel-search{
  position:absolute;
  right:-10rpx;
  top:0;
  bottom:0;
  font-size:32rpx;
  padding:15rpx;
}
.search-textcon .search-text{
  width:80%;
  border:1px solid #4b474c;
  border-radius:10px;
  font-size:28rpx;
  color:#4b474c;
  height:62rpx;
  padding-left:60rpx;
}


.city-tabcon{
  width:100%;
  margin:0 auto;
}
.city-tabcon .tabcon-item{
  width:50%;
  float: left;
}
.city-tabcon .selcity-tablist{
  font-size: 30rpx;
  width:152rpx;
  margin:0 auto;
  height:64rpx;
  line-height: 64rpx;
  text-align: center; 
}


.search-desc{
  margin: 20rpx 30rpx 26rpx;
  font-size:28rpx;
}
.search-tab{
  margin:0 60rpx;
}
.search-tab .search-item{
  width:25%;
  display: inline-block;
  text-align: center;
}
.search-item .item-text{
  border:2rpx solid #f47a20;
  border-radius:30rpx;
  padding:4rpx 16rpx;
  height:40rpx;
  line-height: 44rpx;
  font-size:26rpx;
  color:#f47a20;
}


.show-tab .item-text{
  background:#f47a20;
  color:#ffffff;
}
.search-content .show-content{
  display: block;
}
.search-content .hidden-content{
  display: none;
}

.tab-community {
  margin:50rpx 44rpx 28rpx;
}
.tab-community .community-img{
  width:76rpx;
  height:82rpx;
  float: left;
}

.tab-community .community-name{
  margin-left:36rpx;
  font-size:32rpx;
  color:#4b474c;
  height:44rpx;
  line-height: 92rpx;
}

.empty-datacon{
  margin:134rpx auto;
  width:540rpx;
  height:200rpx;
}
.empty-datacon .empty-desc{
  height:auto;
  line-height: 40rpx;
}
.empty-desclist{
  margin-top:40rpx;
}
.empty-desclist .list-text{
  margin-left:30rpx;
}