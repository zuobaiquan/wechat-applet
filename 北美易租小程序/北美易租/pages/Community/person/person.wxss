.person-info{
  height:auto;
}
.person-info .person-img{
  margin:0 34rpx 36rpx 58rpx;
  float: left; 
  width:136rpx;
  height:136rpx;
  border-radius:50%;
}
.person-info .detail-info{
  margin:30rpx 0;
  float: left;
}
.detail-info .info-nickname{
  font-weight:bold;
  font-size:28px;
  height:50rpx;
  line-height: 50rpx;
}
.detail-info .dot-color{
  margin:0 10rpx;
  color:#f47a20;
}
.detail-info .info-addtime{
  margin-top:10rpx;
  height:34rpx;
  line-height: 34rpx;
  font-size:24rpx;
}



.empty-area{
  background:#f6f6f6;
  width:100%;
  height:26rpx;
}
.swiper-tab{  
    width: 92%;
    margin:0 auto;
    text-align: center; 
    height:64rpx; 
    line-height: 64rpx;
    display: flex;
    justify-content:space-between;
}  
.swiper-tab-list{
    font-size: 30rpx;  
    display: inline-block;  
    padding:0 20rpx;
    color: #777777;  
}  
