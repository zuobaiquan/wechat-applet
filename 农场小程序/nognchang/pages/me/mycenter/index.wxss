/* pages/me/mycenter/index.wxss */
.container .my-center{
    width:100%;
    text-align: center;
    background: #fff;
    position: relative;
}
.my-center .icon-bg{
  width:100%;
  height: 348rpx;
  margin: -4rpx auto 0;
}
.my-center .text{
  font-size: 28rpx;
}
.my-center .person-box{
  position: absolute;
  top:49rpx;
  left:50%;
  transform: translateX(-50%);
  -webkit-transform:translateX(-50%);
}
.my-center .person-box .icon{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 114rpx;
  height: 114rpx;
  margin:0 auto;
}
.my-center .person-box text{
  margin-top: 32rpx;
  display: block;
  font-size: 34rpx;
  color:#fff;
}
.container  .center-list{
  width: 100%;
  border-bottom: 1rpx solid #ddd;
}
.center-list .item{
  margin-top:10rpx;
  width: 32%;
  display:inline-block;
  text-align: center;
  background:#fff;
}
.center-list .item.item-margin{
  margin-left: 2%;
}
.center-list .icon{
    width:150rpx;
    height: 100rpx;
    background:#ddd;
    margin:0 auto;
}
