.container .record-list{
  display: flex;
  justify-content: space-between;
  padding:25rpx;
  background: #fff;
  border-bottom: 1rpx solid #e5e5e5;
}
.record-list .record-left .left-content {
  display: inline-block;
}
.record-list .record-left .left-icon{
  margin-right: 20rpx;
  display: inline-block;
  width: 58rpx;
  height: 58rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.record-list .record-left .left-content .record-title{
  font-size: 30rpx;
  color:#000000;
}
.record-list .record-left .left-content .record-time{
  font-size: 24rpx;
  color:#888888;
}
.record-list text{
  display: block;
}
.record-list .record-right .record-money{
  color:#FF9600;
  font-size: 34rpx;
}
.record-list .record-right .record-status{
  color:#333333;
  font-size: 26rpx;
  text-align: right;
}
.container .norecord-icon{
  width: 134rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  height: 134rpx;
  margin:210rpx auto 43rpx;
}
.container .norecord-text{
  font-size: 32rpx;
  color:#aaaaaa;
  text-align: center;
  display: block;
}
