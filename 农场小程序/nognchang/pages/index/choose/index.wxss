.container .header{
  width: 100%;
  height: 300rpx;
  position: relative;
}
.header .item-image{
  height: 300rpx;
  width: 100%;
}
.dot-box{
  display: flex;
  padding: 40rpx 63rpx;
  justify-content: space-between;
  background: #fff;
}
.dot-box .text{
  color:#333333;
  font-size: 24rpx;
}
.dot-box .text:before{
  margin:-3rpx 20rpx 0 0;
  content: '';
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  vertical-align: middle;
}
.dot-box .text1:before{
  background: #D2EAAF;
}
.dot-box .text2:before{
  background: #90CB49;
}
.dot-box .text3:before{
  background: #E7CEBB;
}
.dot-box .text4:before{
  background: #CFCFCF;
}
.container .tit{
  padding:28rpx 0;
  font-size: 30rpx;
  color:#333333;
}
.container .tit:before{
  content: '';
  margin:-2rpx 24rpx 0 24rpx;
  width: 9rpx;
  height: 33rpx;
  background: #96CB2C;
  display: inline-block;
  vertical-align: middle;
}
.product-box{
  padding:35rpx 35rpx 150rpx 35rpx;
  background: #fff;
}
.product-box image{
  width: 100%;
  margin-bottom: 35rpx;
}
.container .scroll-box{
  overflow-x: auto;
  background: #FFFFFF;
}
.scroll-box .scroll-list{
  padding: 0 24rpx 40rpx;
  width: 100%;
  white-space: nowrap;
}
.scroll-box .scroll-list text{
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-right: 20rpx;
  border-radius: 4rpx;
  border:1rpx solid #e5e5e5;
  color: #969696;
  font-size: 28rpx;
  text-align: center;
  display: inline-block;
  letter-spacing: 8rpx;
}
.scroll-box .scroll-list text.active{
  border:1rpx solid #FF9600;
  color: #FF9600;
}
.product-list{
  display: flex;
  justify-content:flex-start;
  padding:0 18rpx;
  background:#fff;
  flex-wrap:wrap;
}
.product-list .item-list{
  margin-right: 19rpx;
}
.product-list .item-list:nth-child(6n+6){
  margin-right: 0;
}
.product-list .item-list .list{
  width: 105rpx;
  height: 247rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.product-list .item-list .list text{
  color:#fff;
  font-size: 34rpx;
  width: 23rpx;
  line-height: 48rpx;
  word-wrap: break-word;
  margin: 0 auto;
  display: block;
  letter-spacing:23rpx;

}
.product-list .item-list .select-box{
  margin:10rpx 20rpx;
  width: 65rpx;
  height: 65rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.product-list .item-list .unselect-box{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
