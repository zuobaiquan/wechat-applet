.container{
    min-height: 100%;
    background-color: #f7f7f7;
}
.container .img{
    padding: 0rpx 0 26rpx 0;
    margin-top: 45%;
}
.container .img image{
    margin: 0 auto;
    width: 200rpx;
    height: 200rpx;
    display: block;
}
.container .description text{
    display: block;
    padding: 0 0 10rpx 0;
    color: #999999;
    width: 100%;
    text-align: center;
    font-size: 13px;
}
.container .description .h1{
    color: #000;
    font-size: 16px;
    padding-bottom: 30rpx;
}